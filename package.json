{
  "name": "wha-ts-up",
  "version": "0.0.1",
  "description": "Well-typed Helper API - TypeScript UP Bank Package",
  "packageManager": "pnpm@10.11.1",
  "scripts": {
    "lint": "eslint",
    "clean": "rm -rf dist",
    "compile": "tsc --project tsconfig.json",
    "build": "pnpm clean && pnpm compile",
    "copy-release-files": "tsx --tsconfig tsconfig.build.json scripts/copy-release-files.ts",
    "package": "pnpm lint && pnpm build && pnpm run copy-release-files",
    "release": "pnpm package && pnpm publish dist"
  },
  "dependencies": {
    "@ts-rest/core": "3.53.0-rc.1",
    "common-tags": "1.8.2",
    "temporal-polyfill": "0.3.0",
    "zod": "3.25.53"
  },
  "devDependencies": {
    "@eslint/js": "9.28.0",
    "@tsconfig/node24": "24.0.1",
    "@types/common-tags": "1.8.4",
    "@types/node": "22.15.30",
    "eslint": "9.28.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-import-x": "4.15.1",
    "eslint-plugin-prettier": "5.4.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "tsx": "4.19.4",
    "typescript": "5.8.3",
    "typescript-eslint": "8.33.1"
  }
}
